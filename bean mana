            if total == 5 then
                notefruit = "Awaken full skill"
            elseif total >= 1 then
                notefruit = "Đã awaken skill: "..awakenabiliti
            else
                notefruit = ""
            end

            local formatdata = "\nLevel: "..level.."\nInventory: "..wpstorage.."\nMelee: "..Meleehienco.."\nFruit đang ăn: "..foot.." "..notefruit.."\nFruit Store: "..frutidangco.."\nBeli: "..beli.."\nFragment: "..frag
            if MyAccount then
                MyAccount:SetDescription(formatdata.."\nMade By IceLion32#7923")

            end
        end)
    end

end)
